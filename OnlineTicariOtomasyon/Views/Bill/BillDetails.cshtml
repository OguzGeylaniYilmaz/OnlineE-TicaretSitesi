@using OnlineTicariOtomasyon.Models.Classes
@model List<BillNote>
@{
    ViewBag.Title = "BillDetails";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Explanation</th>
        <th>Quantity</th>
        <th>Unit Price</th>
        <th>Amount</th>
    </tr>
    @foreach (var x in Model)
    {
    <tr>
        <td>@x.BillNoteID</td>
        <td>@x.Explanation</td>
        <td>@x.Quantity</td>
        <td>@x.UnitPrice</td>
        <td>@x.Amount</td>
    </tr>
    }
</table>
<a href="/Bill/NewBillNote/" class="btn btn-primary">New Bill Note</a>
